[MOLECULE]
molecule =
    0 1
    O
    H 1 R
    H 1 R 2 A
    R = 1.0
    A = 104.5
    symmetry c1

[QM_PARAM]
method = scf
basis = STO-3G
scf_type = df
reference = rhf
guess =  sad
e_convergence = 1e-8
d_convergence = 1e-8

[MM_PARAM]
pdb_file = 'input.pdb'
forcefield = 'amber99sb.xml'
forcefield_water = 'tip3p.xml
nonbond_method = PME
nonbond_cutoff = 1*nanometer 
constraints = HBonds
temp = 300*kelvin


[QM_MM_PARAM]
qm_atoms = []
qm_residues = []
